using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace org.jiechan.main.models {
    public class accountmodel {

        public string gridctrltext { get; set; }

        public float totalmoney { get; set; }




        public float usable { get; set; }

        public string holdingpercents { get; set; }

        //持仓盈亏
        public float winlose { get; set; }

        public string winlosepercent {
            get {
                return (winlose / totalmoney * 100).ToString("F1");
            }
        }
    }
}
